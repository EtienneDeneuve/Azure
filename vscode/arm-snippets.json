    "Virtual Network Peering": {
        "body": [
            "{",
            "\t\"type\": \"Microsoft.Network/virtualNetworks/virtualNetworkPeerings\",",
            "\t\t\"comments\": \"Be careful, the Vnet to Vnet peering use the name as an ID!\",",
            "\t\"name\": \"[concat(${1:Vnet1}, '/LinkTo', ${2:Vnet2})]\",",
            "\t\"apiVersion\": \"[variables('apiVersion')]\",",
            "\t\"location\": \"[resourceGroup().location]\",",
            "\t\"properties\": {",
            "\t\t\"allowVirtualNetworkAccess\": true,",
            "\t\t\"allowForwardedTraffic\": true,",
            "\t\t\"allowGatewayTransit\": false,",
            "\t\t\"useRemoteGateways\": true,",
            "\t\t\"remoteVirtualNetwork\": {",
            "\t\t\"comments\": \"subscription and resourcesgroupname are optional, but if you want to peer 2 vnet in 2 resources group, you need that! You Need to update the other side, and also need to add \"arm-vnetid-output\" in output :)  \",",
            "\t\t\t\"id\": \"[resourceId(${optional:subscription},${optional:resourceGroupName},'microsoft.network/virtualnetworks', ${2:vnet2})]\"",
            "\t\t}",
            "\t},",
            "\t\"dependsOn\": [",
            "\t\t\"[resourceId(${optional:subscription},${optional:resourceGroupName},'microsoft.network/virtualnetworks', ${2:vnet2})]\"",
            "\t]",
            "},"
        ],
        "description": "Create a vnet peering",
        "prefix": "arm-ed-vn-peering"
    },
